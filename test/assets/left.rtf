\par {\listtext\pard\plain\ltrpar \s39 \rtlch\fcs1 \af0\afs20 \ltrch\fcs0 \f1\fs20\insrsid1732038 \hich\af1\dbch\af0\loch\f1 2.5.\tab}}{\rtlch\fcs1 \af1 \ltrch\fcs0 \insrsid1732038 
